    "keywords": ["Carbon"],
    "type": "library",
    "homepage": "https://github.com/maccotsan/CarbonJp/",
    "license": "MIT",
    "authors": [
        {
            "name": "maccotsan",
            "email": "maccotsan@gmail.com"
        }
